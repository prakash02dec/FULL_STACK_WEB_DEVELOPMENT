<%- include("partials/header") %> 

<h1>Compose </h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label for="posttitle">Title</label>
    <input class="form-control" type="text" name="postTitle">
    </div>
    <div class="form-group">
    <label for="postBody"  class="form-label">Post</label>
    <textarea class="form-control" name="postBody" id="" cols="30" rows="5"></textarea>
    </div>  
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") %> 
